    "execa": "^5.1.1",
    "fs-extra": "^9.1.0",
    "globby": "^11.0.4",
    "immer": "^9.0.6",
    "lru-cache": "^6.0.0",
    "luxon": "^1.28.0",
    "next": "^11.1.1",
    "p-queue": "^6.6.2",
    "prettier": "~2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hotkeys-hook": "^3.4.0",
    "react-is": "^17.0.2",
    "react-query": "^3.19.6",
    "react-use": "^17.2.4",
    "react-window": "^1.8.6",
    "sleep-promise": "^9.1.0",
    "styled-components": "^5.3.0",
    "styled-normalize": "^8.0.7",
    "tmp": "^0.2.1"
  },
