{
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "exe": "ts-node-dev --ignore-watch=. --transpile-only --project=tsconfig.build.json --compiler-options '{\"noEmit\":true}'",
    "exe:dev": "ts-node-dev --watch=.env,next.config.js,package.json,tsconfig*,yarn.lock --ignore-watch=.next --transpile-only --project=tsconfig.build.json --compiler-options '{\"noEmit\":true}'",
    "exe:prod": "NODE_ENV=production node",
    "fix": "yarn fix:eslint; yarn fix:markdownlint; yarn fix:prettier; yarn fix:yarn-deduplicate",
    "fix:eslint": "eslint --fix \"**/*\"",
    "fix:markdownlint": "markdownlint --fix \"**/*\"",
    "fix:prettier": "prettier --write \"**/*\"",
    "fix:yarn-deduplicate": "yarn install && yarn-deduplicate --strategy=fewer && yarn install",
    "lint": "yarn lint:eslint && yarn lint:markdownlint && yarn lint:prettier && yarn lint:tsc && yarn lint:yarn-deduplicate",
    "lint:eslint": "eslint \"**/*\" --report-unused-disable-directives",
    "lint:markdownlint": "markdownlint \"**/*\"",
    "lint:prettier": "prettier --check \"**/*\"",
    "lint:tsc": "tsc --project .",
    "lint:yarn-deduplicate": "yarn-deduplicate --fail --list --strategy=fewer",
    "start": "next start",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "run-if-changed",
      "post-checkout": "run-if-changed",
      "post-merge": "run-if-changed",
      "post-rewrite": "run-if-changed"
    }
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "markdownlint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@types/tmp": "^0.2.0",
    "aws-sdk": "^2.715.0",
    "chalk": "^4.1.0",
    "command-exists": "^1.2.9",
    "copy-to-clipboard": "^3.3.1",
    "envalid": "^6.0.2",
    "execa": "^4.0.3",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.1",
    "graphql": "^15.3.0",
    "immer": "^7.0.5",
    "lru-cache": "^6.0.0",
    "luxon": "^1.24.1",
    "next": "^9.4.4",
    "p-queue": "^6.5.0",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hotkeys-hook": "^2.1.4",
    "react-is": "^16.13.1",
    "react-query": "^2.5.4",
    "react-use": "^15.3.2",
    "react-window": "^1.8.5",
    "sleep-promise": "^8.0.1",
    "styled-components": "^5.1.1",
    "styled-normalize": "^8.0.7",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.10.4",
    "@hkdobrev/run-if-changed": "^0.3.1",
    "@kachkaev/commands": "^0.1.0",
    "@kachkaev/eslint-config-react": "^0.1.1",
    "@kachkaev/markdownlint-config": "^0.1.0",
    "@kachkaev/prettier-config": "^1.0.0",
    "@next/bundle-analyzer": "^9.4.4",
    "@types/command-exists": "^1.2.0",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.5",
    "@types/lodash": "^4.14.157",
    "@types/lru-cache": "^5.1.0",
    "@types/luxon": "^1.24.1",
    "@types/node": "^14.0.23",
    "@types/node-fetch": "^2.5.7",
    "@types/nprogress": "^0.2.0",
    "@types/object-hash": "^1.3.3",
    "@types/react": "^16.9.43",
    "@types/react-query": "^1.1.2",
    "@types/react-window": "^1.8.2",
    "@types/styled-components": "^5.1.1",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^7.5.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "lodash": "^4.17.19",
    "markdownlint-cli": "^0.23.2",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^1.4.0",
    "ts-node-dev": "^1.0.0-pre.52",
    "typescript": "^3.9.7",
    "yarn-deduplicate": "^2.1.1"
  },
  "engines": {
    "node": ">=12.15.0",
    "yarn": ">=1.22.0"
  },
  "run-if-changed": {
    "yarn.lock": "yarn install --prefer-offline --pure-lockfile"
  }
}
