{
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "exe": "ts-node-dev --ignore-watch=. --transpile-only --project=tsconfig.build.json --compiler-options '{\"noEmit\":true}'",
    "exe:dev": "ts-node-dev --watch=.env,next.config.js,package.json,tsconfig*,yarn.lock --ignore-watch=.next --transpile-only --project=tsconfig.build.json --compiler-options '{\"noEmit\":true}'",
    "exe:prod": "NODE_ENV=production node",
    "fix": "yarn fix:eslint; yarn fix:prettier; yarn fix:yarn-deduplicate",
    "fix:eslint": "eslint --fix \"**/*\"",
    "fix:prettier": "prettier --write .",
    "fix:yarn-deduplicate": "yarn install && yarn-deduplicate --strategy=fewer && yarn install",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:tsc && yarn lint:yarn-deduplicate",
    "lint:eslint": "eslint \"**/*\" --report-unused-disable-directives",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit --project .",
    "lint:yarn-deduplicate": "yarn-deduplicate --fail --list --strategy=fewer",
    "start": "next start",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "run-if-changed",
      "post-checkout": "run-if-changed",
      "post-merge": "run-if-changed",
      "post-rewrite": "run-if-changed"
    }
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": "./eslint.config.js"
  },
  "dependencies": {
    "@types/tmp": "^0.1.0",
    "chalk": "^4.0.0",
    "command-exists": "^1.2.8",
    "envalid": "^6.0.1",
    "execa": "^4.0.0",
    "fs-extra": "^9.0.0",
    "globby": "^11.0.0",
    "graphql": "^15.0.0",
    "immer": "^6.0.3",
    "luxon": "^1.23.0",
    "next": "^9.3.4",
    "p-queue": "^6.3.0",
    "prettier": "^2.0.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "styled-components": "^5.0.1",
    "styled-normalize": "^8.0.7",
    "tmp": "^0.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@hkdobrev/run-if-changed": "^0.3.1",
    "@next/bundle-analyzer": "^9.3.4",
    "@types/command-exists": "^1.2.0",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/lru-cache": "^5.1.0",
    "@types/luxon": "^1.22.0",
    "@types/node": "^13.11.0",
    "@types/node-fetch": "^2.5.5",
    "@types/nprogress": "^0.2.0",
    "@types/object-hash": "^1.3.1",
    "@types/prettier": "^2.0.0",
    "@types/react": "^16.9.32",
    "@types/styled-components": "4.1.19",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "babel-plugin-styled-components": "^1.10.7",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "husky": "^4.2.3",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.1",
    "lodash": "^4.17.15",
    "next-compose-plugins": "^2.2.0",
    "next-fonts": "^1.0.3",
    "prettier-plugin-packagejson": "^2.1.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3",
    "yarn-deduplicate": "^2.0.0"
  },
  "engines": {
    "node": ">=12.15.0",
    "yarn": ">=1.22.0"
  },
  "run-if-changed": {
    "yarn.lock": "yarn install --prefer-offline --pure-lockfile"
  }
}
